<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.propertymanagement">

    <application
        android:allowBackup="true"
        android:label="Property Management"
        android:icon="@mipmap/ic_launcher"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".ui.login.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".ui.owner.OwnerDashboardActivity" />
        <activity android:name=".ui.tenant.TenantDashboardActivity" />
        <activity android:name=".ui.property.PropertyDetailsActivity" />
        <activity android:name=".ui.payment.PaymentReceiptActivity" />

    </application>

</manifest>
